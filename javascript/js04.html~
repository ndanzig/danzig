//change an image on mouse over
var img1a=new Image();
img1a.src="../compass.gif";
var img1b=new Image();
img1b.src="../earth.gif";
function mouseover() {
  document.img1.src=img1a.src;
}
function mouseout() {
  document.img1.src=img1b.src;
}
